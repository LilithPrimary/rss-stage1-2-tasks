{
  "compilerOptions": {
    "target": "es2016",
    "baseUrl": "./src",
    "outDir": "./dist",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": [
    "./src"
  ]
}